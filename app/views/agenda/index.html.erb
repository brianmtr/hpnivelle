<table>

  <thead>
   
    <tr>
      <th>Title</th>
      <th>Date</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>
<% @events.each do |event| %>
  <td><% if event.status === "accepted" %>
    <tbody>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.date %></td>
        <td><%= link_to 'Voir événement', event_path(event.id) %></td>
      </tr>
      <% else %>
        <% end %>
    <% end %>
  </tbody>
</table>



